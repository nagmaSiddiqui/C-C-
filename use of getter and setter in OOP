#include <iostream>

using namespace std;
class Hero{
    private:
    int health;
    public:
    char level;
    
    void print(){
        cout<<level<<endl;
    }
    
    int getHealth(){
        return health;
    }
    int getlevel(){
        return level;
    }
    
    void setHealth(int h){
        health=h;
    }
    void setlevel(char ch){
        level=ch;
    }

};
int main()
{
    
Hero ramesh;
cout<<"Ramesh health:"<<ramesh.getHealth()<<endl;
//ramesh.health=70;
ramesh.level='A';
//cout<<"health is:"<<ramesh.health<<endl;
cout<<"level is:"<<ramesh.level<<endl;

